<AssignMessage name="AM-SetTargetErrVar">
  <DisplayName>AM-SetTargetErrVar</DisplayName>
  <AssignVariable>
    <Name>siclient.error.code</Name>
    <Ref>response.status.code</Ref>
  </AssignVariable>
  <AssignVariable>
    <Name>siclient.error.message</Name>
    <Ref>response.status.phrase</Ref>
  </AssignVariable>
  <AssignVariable>
    <Name>siclient.error.info</Name>
    <Value>https://illiadev93-eval-si.apigee.io</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>siclient.error.status</Name>
    <Ref>response.status.code</Ref>
  </AssignVariable>
  <AssignVariable>
    <Name>siclient.error.reason</Name>
    <Ref>response.status.phrase</Ref>
  </AssignVariable>
</AssignMessage>