<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="EV-ExtractTokenJSON">
    <JSONPayload>
        <Variable name="token">
            <JSONPath>$.token</JSONPath>
        </Variable>
    </JSONPayload>
    <VariablePrefix>private.jwt</VariablePrefix>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>