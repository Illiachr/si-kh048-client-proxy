<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-SetJSONThreatErrVar">
    <DisplayName>AM-SetJSONThreatErrVar</DisplayName>
    <AssignVariable>
        <Name>jsonThreatErrorRegex</Name>
        <Value>JSONThreatProtection\[JTP-Protect\]\: Execution failed. reason\: JSONThreatProtection\[JTP-Protect\]\: </Value>
    </AssignVariable>
    <AssignVariable>
        <Name>siclient.error.code</Name>
        <Value>400</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>siclient.error.message</Name>
        <Template>Invalid JSON payload: {replaceFirst(error.message,jsonThreatErrorRegex,'')}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>siclient.error.info</Name>
        <Value>https://illiadev93-eval-si.apigee.io</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>siclient.error.status</Name>
        <Value>400</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>siclient.error.reason</Name>
        <Value>Bad Request</Value>
    </AssignVariable>
</AssignMessage>