<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations mapIdentifier="ClientAuth" async="false" continueOnError="false" enabled="true" name="KVM-GetToken">
  <DisplayName>KVM-GetToken</DisplayName>
  <ExpiryTimeInSecs>60</ExpiryTimeInSecs>
  <Scope>environment</Scope>
  <Get assignTo="jwt.token">
    <Key>
      <Parameter>accesstoken.CUID</Parameter>
    </Key>
  </Get>
</KeyValueMapOperations>