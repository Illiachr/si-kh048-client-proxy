<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-401TokenInvalid">
    <DisplayName>RF-401TokenInvalid</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers>
            </Headers>  
            <Payload contentType="application/json">{error.content}</Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized - {fault.name}</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>