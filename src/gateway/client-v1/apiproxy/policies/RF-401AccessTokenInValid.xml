<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-401AccessTokenInValid">
    <DisplayName>RF-401AccessTokenInValid</DisplayName>
    <FaultResponse>
        <Set>
            <Headers/>
              <Payload contentType="application/json">
                {
                  "error":"Accsess token not valid or expired"
                }
              </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>